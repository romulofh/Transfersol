<h1>HI MOTHERFUCKER!</h1>

<div class="panel-body">

<%= form_for @agency do |f| %>

    <div class="form-group">
        <%= f.label :number %>
        <%= f.text_field :number, autofocus: true, class: 'form-control' %>
    </div>
    <div class="form-group">
        <%= f.label :address %>
        <%= f.text_field :address, autofocus: true, class: 'form-control' %>
    </div>

    <%= f.submit class: 'btn btn-primary' %>

<% end %>

</div>